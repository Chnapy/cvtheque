{{ form_start(form, {'method': 'post', 'action': path('mg_admin_registration'), 'attr': {'class': 'fos_user_registration_register'}}) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword) }}
    {{ form_row(form.firstname) }}
    {{ form_row(form.lastname) }}
    {{ form_row(form.address.street) }}
    {{ form_row(form.address.zipCode) }}
    {{ form_row(form.address.town) }}
    {{ form_row(form.address.country) }}
    {{ form_row(form.phoneNumber) }}
    <input type="hidden" name="_csrf_token"
        value="{{ csrf_token('authenticate') }}"
    >
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
{{ form_end(form) }}